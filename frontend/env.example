# PoolFi Frontend Environment Variables
# Copy this file to .env.local for local development
# Set these values in Vercel dashboard for production deployment

# Smart Contract Addresses (Update after deployment)
NEXT_PUBLIC_POOL_MANAGER_ADDRESS=0xd9145CCE52D386f254917e481eB44e9943F39138

# WalletConnect Project ID (Get from https://cloud.walletconnect.com/)
# Required for wallet connection functionality
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=e621e0027ef5a2a1afdcc6351101a95c

# Reef Network Configuration
# For production, use the main Reef network RPC
NEXT_PUBLIC_REEF_RPC_URL=https://rpc.reefscan.com
NEXT_PUBLIC_REEF_CHAIN_ID=13939

# Optional: Analytics and monitoring
# NEXT_PUBLIC_GA_ID=your_google_analytics_id
# NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn

# Production optimizations
NODE_ENV=production
NEXT_TELEMETRY_DISABLED=1
